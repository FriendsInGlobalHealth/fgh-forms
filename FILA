  <script type="text/javascript">
  // Keys are regimen concept IDs
  var formulations = {
    1703: [ "Drug:40", "Drug:22", "Drug:39" ],
    6100: [ "Drug:28", "Drug:22", "Drug:23", "Drug:27", "Drug:29", "Drug:30" ],
